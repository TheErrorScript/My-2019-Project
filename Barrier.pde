class Barrier {
   
   //barrier coords
   //x coord
   float x;
   //y coord
   float y;
   
   //width and height of the barrier
   //width
   float w;
   //height
   float h;
   
   //constructor
   Barrier(float tempX, float tempY, float tempW, float tempH) {
      x = tempX;
      y = tempY;
      w = tempW;
      h = tempH;
   }
}
